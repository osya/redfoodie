<script id="business-reviews-template" type="text/template">
    <div>
        <div class="item-review-container user-restaurant-review" data-rf-user-restaurant-review="<%=review_number%>">
            <% var loginUser    =   '3435' %>
            <div class="row">
                <div class="col-md-2">
                    <a href="/users/<%= reviewer.username%>/profile">
                        <img src="<%=userProfileImage(reviewer,55,55,App.basePath)%>">
                    </a>
                </div>
                <div class="col-md-8">
                    <span>
                        <a href="/users/<%= reviewer.username%>/profile"><%=reviewer.first_name%></a>
                    </span>

                    <div class="user-stats">
                        <i class="fav"></i>
                        <span class="user-stat"><%=reviewer.review%></span>
                        <i class="fa fa-user-plus" style="color: #9298A5;"></i>
                        <span class="user-stat"><%=reviewer.followers_count%></span>
                    </div>
                </div>
                <div class="col-md-2">
                    <% if(visited){ %>
                    Visited this place
                    <% }else{ %>
                    Never been there
                    <% } %>
                </div>
            </div>

            <div class="row">
                <div class="col-md-2">
                </div>
                <div class="col-md-2">
                    <% if(loginUser != reviewer.id ) { %>
                    <div class="clearfix user-rank pull-right page-user-rank">
                        <% if((reviewer.is_followed) && (typeof(reviewer.is_followed)!='undefined')){ %>
                        <div class="button pull-left followed following">
                            <a href="javascript:void(0);" id="follow"
                               class="anim-top pos_relative following"
                               data-user-id="<%=reviewer.id%>">Following</a>
                        </div>
                        <% }else{ %>
                        <div class="button pull-left followed follow">
                            <a href="javascript:void(0);" id="follow"
                               class="anim-top pos_relative follow"
                               data-user-id="<%=reviewer.id%>">Follow</a>
                        </div>
                        <% } %>
                    </div>
                    <% } %>
                </div>
                <div class="col-md-5">
                </div>
                <div class="col-md-3">
                    <div class="page-review-date">
                        <%=created%>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-2">
                </div>

                <div class="col-md-10">
                    <% var reviewText = review_text.replace(/\n/g, "<br />"); %>
                    <p style="overflow-wrap: break-word;"><%=reviewText%></p>
                    <% if(review_images && review_images.length > 0) { %>
                    <div class="review-photos-container clearfix pull-left">
                        <%
                        _.each(review_images,function(reviewImage){
                        %>
                        <a data-image-id="<%=reviewImage.id%>" onclick="openSlider(<%=reviewImage.id%>);"
                           href="javascript:void(0);">
                            <img src="/uploads/thumbs/business/<%=busines.slug %>/100x100_<%=reviewImage.path %>"
                                 height="80px" width="80px" />
                        </a>
                        <%
                        });
                        %>
                    </div>
                    <% } %>


                    <% if(management_resp.length > 0 ){ %>
                    <div class="criteria">
                        <% _.each(management_resp,function(resp){ %>
                        <% if (resp.management_resp=='management'){ %>
                        <p>
                            <strong>Management Response :</strong>
                            <%=resp.comment%>
                        </p>
                        <% }else{ %>
                        <p>
                            <strong><%= resp.commenter.first_name%> :</strong>
                            <%=resp.comment%>
                        </p>
                        <% } %>
                        <% }); %>
                    </div>
                    <% } %>

                    <div class="reviewCommentsLists">
                        <%
                        var commentText;
                        _.each(reviewcomments,function(comment,key){
                        commentText = comment.comment;
                        if(key>2)return;

                        %>
                        <div class="wrt_Coment-box clearfix mgn_t10">
                            <div class="pull-left">
                                <a href="<%=App.baseUrl%>/users/<%= comment.commenter.username%>/profile">
                                    <% if(comment.profile_image){ %>
                                    <img src="<%=comment.profile_image%>" width="34" height="34">
                                </a>
                                <% }else{ %>
                                <img src="<%=userProfileImage(comment.commenter,34,33,App.basePath)%>" width="34"
                                     height="34"></a>
                                <% } %>
                            </div>
                            <div class="pull-left width_625 pad_l5">
                                <p>
                                    <a href="/users/<%= comment.commenter.username%>/profile"><%=comment.commenter.first_name%> </a>
                                    <%=commentText%>
                                </p>
                                <span class="timeago" title="<%= comment.date_posted %>"> <%= comment.date_posted %> </span>
                                <% if(loginUser == comment.commenter.id){ %>
                                <br /><span class="delete-user-comment-user"><a href="#">Delete Comment</a></span><span class="delete-comment-confirm" style="margin-left: 5px;display: none;">
                                    &nbspAre you sure?&nbsp<a class="confirm-yes" href="#" data-rf-commented-review="<%=comment.id%>">Yes</a>&nbsp<a class="confirm-no" href="#"
                                                                                                                                                     data-rf-commented-review="<%=comment.id%>">No</a>
                                </span>
                                <% } %>
                            </div>
                        </div>
                        <%
                        });
                        %>
                    </div>
                </div>
            </div>


            <div class="row" style="font-size: 13px; color: #8d8d8d; font-weight: 600;margin-top: 7px;">
                <div class="col-md-2"></div>
                <div class="col-md-7">
                </div>
                <div class="col-md-2 pull-right">
                    <% if ((loginUser != '') && (loginUser == reviewer.id)) { %>
                    <a href="#" data-rf-user-restaurant-review-id="<%=review_number%>"
                       class="edit-user-restaurant-review page-review-action-item">Edit</a>
                    <a href="#"
                       data-rf-restaurant-review-id="<%=review_number%>"
                       data-rf-review-restaurant="<%=busines.slug%>"
                       data-rf-tab-status="#reviews"
                       class="delete-user-restaurant-review page-review-action-item"><i style="border: solid 1px;padding: 1px;" class="fa fa-trash"></i></a>
                    <% } %>
                </div>
            </div>
            <hr style="margin: 10px 10px 10px 84px;">


            <div class="row" style="font-size: 13px; color: #8d8d8d; font-weight: 600;">
                <div class="col-md-2"></div>
                <div class="col-md-10">
                    <div>
                        <div class="pull-left review-status page-review-status">
                            <a data-original-title="Useful" rel="tooltip" class="use" id="useful-<%=review_number%>" href="javascript:void(0);" onclick="usefulcall(<%=review_number%>, $(this))"><span class="fa fa-smile-o"></span> Useful</a>
                            <span class="reviewUsefulCount"><%=reviewuseful.length%></span>
                        </div>

                        <div class="pull-right login-user-act page-business-share-container">
                            <span style="position:relative;">
                                <a class="Share share_to" href="#"><span class="fa fa-share-square-o"></span> Share</a>
                                <% var reviewText   =   review_text.substr(0,100);
                                var photo    =   '';
                                photo   =   "https://www.redfoodie.com/uploads/thumbs/business/rodeo-connaught-place-new-delhi/80x80_IMG_20151201_151221098-rodeo-connaught-place-new-delhi-4e709125ffc4499be0f99fd7e683b5c9.jpg";
                                %>

                                <div class="profile_social_share" style="display: none;">
                                    <a href="javascript:void(0)">
                                        <span data-original-title="facebook" rel="tooltip" class="social share-fb"
                                              data-res-name="Rodeo, Connaught Place, Delhi NCR "
                                              data-url="https://www.redfoodie.com/reviews/rodeo-connaught-place-new-delhi/__REVIEW_ID__/show"
                                              data-review-snippet="<%=reviewText%>"
                                              data-photo="<%=photo%>"
                                              data-user-name="<%=reviewer.first_name%>">
                                            <span class="fa fa-facebook" style="margin-right: 0px;"></span>
                                        </span>
                                    </a>
                                    <a data-original-title="twitter" rel="tooltip" class="review-share-twitter"
                                       href="/share/tweet/__USER__/__BUSINESS__/__REVIEW_ID__/reviews_show/user/user-share"
                                       <span class="fa fa-twitter"></span>
                                   </a>
                                </div>
                            </span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <hr style=" width 512px; margin-left -16px; ">
    </div>
</script>
