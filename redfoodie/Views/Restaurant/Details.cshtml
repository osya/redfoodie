@section Scripts
{
    <script>
        function initMap() {
            var gMapIcons = {
                end: '/redfoodie/img/google-map/pin.png',
                start: '/redfoodie/img/google-map/gmap-pin-common.png'
            };
            var gMap = utils.plotMap(@Model.Location.Latitude, @Model.Location.Longitude, 'business-map-container', 11);
            utils.addMapMarker(@Model.Location.Latitude, @Model.Location.Longitude, gMap, 'Rodeo', 'Rodeo', gMapIcons.end, false);

            $('#map-modal')
                .modal({
                    show: false
                })
                .on("shown.bs.modal",
                    function() {
                        google.maps.event.trigger(gMap, "resize");
                        gMap.setCenter(new google.maps.LatLng(@Model.Location.Latitude, @Model.Location.Longitude));
                    });
        }
    </script>
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?v=3.exp&key=@googleApiKey&libraries=places&callback=initMap" async defer></script>
    @Scripts.Render("~/Content/restaurant-details/js")
}