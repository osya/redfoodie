@model redfoodie.Models.UserViewModel
@{
    var baseUri = new Uri("http://myredfoodie.com:8080");
    // ReSharper disable once UseStringInterpolation
    var profileUri = new Uri(baseUri, string.Format("/Manage/ViewProfile?userId={0}", Model.Id));
}
<style>
    td { padding: 0; }   
    table {
        border-collapse: collapse;      
    }
</style>
<div data-x-div-type="html" xmlns="http://www.w3.org/1999/xhtml">
    <div data-x-div-type="body" style="background-color: #eeeeee;margin: 0;padding: 0;background-color: #eeeeee">
        ,
        <table style="background-color: #eeeeee; width: 100%;">
            <tbody>
                <tr style="background-color: #363636">
                    <td style="vertical-align:middle;">
                        <table width="542" style="border: 0; margin-left: auto; margin-right: auto;">
                            <tbody>
                            <tr>
                                <td style="padding: 10px;vertical-align:middle;text-align:left;" awidth="129">
                                    <img src="data:image/png;base64," alt="Redfoodie Logo"/>
                                </td>
                                <td style="padding: 15px 0;vertical-align:middle;text-align:right;" width="490">
                                    <a href="https://www.facebook.com/Redfoodiedotcom" style="padding-right: 5px" target="_blank" tabindex="-1" rel="external">
                                        <img src="data:image/png;base64," alt="Facebook"/>
                                    </a><a href="https://www.twitter.com/RedfoodieApp" target="_blank" tabindex="-1" rel="external">
                                        <img src="data:image/png;base64," alt="Twitter"/>
                                    </a>
                                </td>
                            </tr>
                            </tbody>
                        </table>
                        <table style="background-color: #d94a39;border-radius: 5px;border-bottom-left-radius: 0; border-bottom-right-radius: 0;margin-left: auto; margin-right: auto;" width="542"><tbody><tr><td style="font-family: Gotham, Helvetica, sans-serif;font-size: 18px;font-weight: bold;color: #fff; padding: 10px; text-align: center;">Friend Invitation From Redfoodie!</td></tr></tbody></table>
                    </td>
                </tr>
            </tbody>
        </table>

        <table style="font-family: Arial, Helvetica, sans-serif;" width="500">
            <tbody>
                <tr>
                    <td style="background: #eaeaea; background-color: #eaeaea;vertical-align:top;text-align:left;">
                        <table style="background: #ffffff;padding: 0 0 25px; background-color: #ffffff;" width="500">
                            <tbody>
                                <tr>
                                    <td style="font-family: Arial, Helvetica Neue; padding: 10px 20px;vertical-align:top;text-align:left;">
                                        <div style="font-size: 14px;color: #5b5b5b;vertical-align: middle;display: block;padding: 15px 0;border-bottom: 1px solid #e4e4e4">
                                            <div style="text-align: center;" >
                                                <a href="@profileUri" target="_blank" tabindex="-1" rel="external">
                                                    <img src="@(new Uri(baseUri, Model.ImageFullFileName))" alt="@Model.UserName" title="@Model.UserName">
                                                </a>
                                            </div>
                                            <div style="text-align: center;" >You have been invited to Redfoodie by <a href="@profileUri" style="color: #D94A38;font-size: 25px;font-weight: 600;text-decoration: none" target="_blank" tabindex="-1" rel="external">@Model.UserName</a></div>
                                        </div>
                                    </td>
                                </tr>
                                <tr>
                                    <td style="vertical-align:top; text-align: center;">
                                        <span style="font-size: 14px;color: #5b5b5b;display: block;line-height: 40px">
                                            You can follow @Model.UserName by visiting their 
                                            <a href="@profileUri" style="color: #D94A38;font-size: 14px;font-weight: 600;text-decoration: none" target="_blank" tabindex="-1" rel="external">profile</a>, and get their activity updates.
                                        </span><table style="font-family: Arial, Helvetica Neue; border: 0;" width="541">
                                            <tbody>
                                                <tr style="padding: 10px 20px">
                                                    <td style="font-family: Arial, Helvetica Neue;vertical-align:top;">
                                                        <a href="@profileUri" style="background: #db4a39;color: #fff;padding: 5px 10px;display: block;width: 235px;text-decoration: none;font-size: 14px; font-weight: bold;margin-left: auto; margin-right: auto;" target="_blank" tabindex="-1" rel="external">View Profile</a>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                </tr>
                <tr>
                    <td style="font-size: 12px; color: #2a2a28;vertical-align:middle;text-align:center;" height="35">
                        <div style="text-align: center;" >© 2014. Redfoodie Pvt. Ltd. All rights reserved. Redfoodie</div>
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
</div>